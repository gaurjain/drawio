        <mxCell id="0AIhybsjH3-FPDcnxdHe-1" value="&lt;b&gt;&lt;font color=&quot;#ff3333&quot;&gt;&lt;span style=&quot;font-size: 18px&quot;&gt;&lt;i&gt;&lt;br&gt;Your application&lt;br&gt;organized as&lt;br&gt;verticle instances&lt;/i&gt;&lt;/span&gt;&lt;/font&gt;&lt;/b&gt;&lt;b&gt;&lt;br&gt;&lt;/b&gt;" style="ellipse;whiteSpace=wrap;html=1;sketch=1;fillColor=#ffcd28;strokeColor=none;fontFamily=Comic Sans MS;" parent="0uA7_dlgTEhDihHVL7zL-1" vertex="1">
