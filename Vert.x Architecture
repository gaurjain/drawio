          <mxGeometry x="837.9999694824219" y="600.9966870117188" width="439" height="147" as="geometry" />
        </mxCell>
        <mxCell id="_UWY_-RnWUwg3JTMprNR-2" value="&lt;meta charset=&quot;utf-8&quot;&gt;&lt;pre class=&quot;highlight&quot; style=&quot;box-sizing: inherit ; font-family: &amp;quot;roboto mono&amp;quot; , , &amp;quot;menlo&amp;quot; , &amp;quot;monaco&amp;quot; , &amp;quot;consolas&amp;quot; , &amp;quot;liberation mono&amp;quot; , &amp;quot;courier new&amp;quot; , monospace ; font-size: 0.875rem ; display: block ; overflow-x: auto ; padding: 1rem ; background: rgb(58 , 40 , 64) ; color: rgb(255 , 255 , 255) ; border-radius: 0.8rem ; margin: 1.5rem 0px ; font-style: normal ; font-weight: 300 ; letter-spacing: normal ; text-indent: 0px ; text-transform: none ; word-spacing: 0px&quot;&gt;&lt;code class=&quot;language-java&quot; style=&quot;box-sizing: inherit ; font-family: &amp;quot;roboto mono&amp;quot; , , &amp;quot;menlo&amp;quot; , &amp;quot;monaco&amp;quot; , &amp;quot;consolas&amp;quot; , &amp;quot;liberation mono&amp;quot; , &amp;quot;courier new&amp;quot; , monospace ; font-size: 1em&quot;&gt;server.requestHandler(request -&amp;gt; {&#xa;  &lt;span class=&quot;hljs-comment&quot; style=&quot;box-sizing: inherit ; color: rgb(173 , 181 , 189)&quot;&gt;// This handler will be called every time an HTTP request is received at the server&lt;/span&gt;&#xa;  request.response().end(&lt;span class=&quot;hljs-string&quot; style=&quot;box-sizing: inherit ; color: rgb(108 , 185 , 240)&quot;&gt;&quot;hello world!&quot;&lt;/span&gt;);&#xa;});&lt;/code&gt;&lt;/pre&gt;" style="text;whiteSpace=wrap;html=1;" parent="1" vertex="1">
          <mxGeometry x="837.9999694824219" y="715.9966870117188" width="540" height="161" as="geometry" />
        </mxCell>
        <mxCell id="_UWY_-RnWUwg3JTMprNR-3" value="&lt;div class=&quot;paragraph&quot; style=&quot;box-sizing: inherit ; color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;&lt;p style=&quot;box-sizing: inherit&quot;&gt;When Vert.x has an event available to pass,&amp;nbsp;&amp;nbsp;&lt;span&gt;Vert.x uses a NIO thread, called event-loop, to call the event-handler&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;box-sizing: inherit ; font-weight: bolder&quot;&gt;asynchronously&lt;/span&gt;&lt;span&gt;.&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;paragraph&quot; style=&quot;box-sizing: inherit ; color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="1" vertex="1">
          <mxGeometry x="-176" y="726" width="1063" height="66" as="geometry" />
        </mxCell>
        <mxCell id="_UWY_-RnWUwg3JTMprNR-4" value="&lt;div style=&quot;text-align: justify&quot;&gt;&lt;span style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;Generally, Vert.x does not block the calling thread.&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px ; text-align: justify&quot;&gt;&lt;span&gt;If a result can be provided immediately,&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px ; text-align: justify&quot;&gt;&lt;span&gt;it will be returned immediately,&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px ; text-align: justify&quot;&gt;&lt;span&gt;otherwise calling verticle will usually provide a handler&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px ; text-align: justify&quot;&gt;&lt;span&gt;to receive events some time later.&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;text-align: justify&quot;&gt;&lt;font color=&quot;#3c3c3b&quot; face=&quot;roboto, sans-serif&quot;&gt;&lt;span style=&quot;font-size: 18px&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="1" vertex="1">
          <mxGeometry x="-757" y="837" width="440" height="86" as="geometry" />
        </mxCell>
        <mxCell id="_UWY_-RnWUwg3JTMprNR-5" value="&lt;span style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;Verticle handle events by providing&amp;nbsp;&lt;/span&gt;&lt;em style=&quot;box-sizing: inherit ; color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;handlers&lt;/em&gt;&lt;span style=&quot;color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;&amp;nbsp;to the Vert.x APIs.&amp;nbsp;&lt;/span&gt;" style="ellipse;whiteSpace=wrap;html=1;fillColor=#76608a;strokeColor=#432D57;fontColor=#ffffff;" parent="1" vertex="1">
          <mxGeometry x="562.83" y="352" width="360.5" height="86" as="geometry" />
        </mxCell>
        <mxCell id="_UWY_-RnWUwg3JTMprNR-6" value="&lt;div class=&quot;sect1&quot; style=&quot;box-sizing: inherit ; color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;&lt;div class=&quot;sectionbody&quot; style=&quot;box-sizing: inherit&quot;&gt;&lt;div class=&quot;paragraph&quot; style=&quot;box-sizing: inherit&quot;&gt;&lt;p style=&quot;text-align: left ; box-sizing: inherit&quot;&gt;&lt;/p&gt;&lt;ol&gt;&lt;li style=&quot;text-align: left&quot;&gt;This means that if you want a lot of concurrency using blocking APIs then you need a lot of threads to prevent your application grinding to a halt.&lt;/li&gt;&lt;li style=&quot;text-align: left&quot;&gt;Threads have overhead in terms of the memory they require (e.g. for their stack) and in context switching.&lt;/li&gt;&lt;li style=&quot;text-align: left&quot;&gt;For the levels of concurrency required in many modern applications, a blocking approach just doesn’t scale.&lt;/li&gt;&lt;/ol&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;sect1&quot; style=&quot;box-sizing: inherit ; color: rgb(60 , 60 , 59) ; font-family: &amp;#34;roboto&amp;#34; , sans-serif ; font-size: 18px&quot;&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="1" vertex="1">
          <mxGeometry x="-756.5" y="949" width="1194" height="80" as="geometry" />
        </mxCell>
