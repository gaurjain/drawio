        <mxCell id="-e6cO6g6VIMD7RvrtqLD-119" value="" style="rounded=1;whiteSpace=wrap;html=1;strokeColor=#b85450;fillColor=none;strokeWidth=1;fontFamily=Comic Sans MS;" parent="0uA7_dlgTEhDihHVL7zL-1" vertex="1">
          <mxGeometry x="-855.0040054988799" y="-1768.458936399983" width="146.39" height="73.42" as="geometry" />
