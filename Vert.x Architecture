        <mxCell id="ZN7Vy6yDlq3aeb59_Ysi-2" value="&lt;h1&gt;&lt;font color=&quot;#ff6666&quot;&gt;NetServer&lt;/font&gt;&lt;/h1&gt;&lt;p&gt;&lt;span style=&quot;color: rgb(71 , 71 , 71) ; font-size: 14px ; background-color: rgb(255 , 255 , 255)&quot;&gt;Is a TCP server.&lt;/span&gt;&lt;br&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;fontFamily=Comic Sans MS;" parent="1" vertex="1">
          <mxGeometry x="1196.5" y="190" width="347" height="90" as="geometry" />
        </mxCell>
        <mxCell id="ZN7Vy6yDlq3aeb59_Ysi-3" value="" style="endArrow=none;dashed=1;html=1;dashPattern=1 3;strokeWidth=1;exitX=0.607;exitY=0.994;exitDx=0;exitDy=0;exitPerimeter=0;entryX=0.357;entryY=0.744;entryDx=0;entryDy=0;entryPerimeter=0;fontFamily=Comic Sans MS;" parent="1" source="LDTQbZ4eaFygSTIggesX-43" target="ZN7Vy6yDlq3aeb59_Ysi-2" edge="1">
          <mxGeometry width="50" height="50" relative="1" as="geometry">
            <mxPoint x="1820.3400000000001" y="-116.98000000000002" as="sourcePoint" />
            <mxPoint x="1493.4999999999998" y="-374" as="targetPoint" />
