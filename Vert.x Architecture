        <mxCell id="-e6cO6g6VIMD7RvrtqLD-124" value="&lt;div&gt;&lt;b style=&quot;font-size: 20px ; color: rgb(82 , 148 , 207)&quot;&gt;A promise has a future() method&lt;/b&gt;&lt;/div&gt;&lt;font style=&quot;font-size: 20px&quot; color=&quot;#5294cf&quot;&gt;&lt;div&gt;&lt;b&gt;which eventually holds the result of an&lt;/b&gt;&lt;/div&gt;&lt;b&gt;&lt;div&gt;&lt;b&gt;asynchronous operation.&lt;/b&gt;&lt;/div&gt;&lt;/b&gt;&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="0uA7_dlgTEhDihHVL7zL-1" vertex="1">
          <mxGeometry x="-617" y="-1285.5" width="378" height="46" as="geometry" />
