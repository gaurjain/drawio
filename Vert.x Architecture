        <mxCell id="-e6cO6g6VIMD7RvrtqLD-26" value="Landscaper Verticle" style="shape=image;html=1;verticalAlign=top;verticalLabelPosition=bottom;labelBackgroundColor=none;imageAspect=0;aspect=fixed;image=https://cdn0.iconfinder.com/data/icons/ikooni-outline-free-basic/128/seo_fun-05-128.png;fontFamily=Comic Sans MS;" parent="0uA7_dlgTEhDihHVL7zL-1" vertex="1">
