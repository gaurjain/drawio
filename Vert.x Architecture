        <mxCell id="DnLwF9LTj5ysab_OE89t-1" value=":Demultiplexer" style="shape=umlLifeline;perimeter=lifelinePerimeter;whiteSpace=wrap;html=1;container=1;collapsible=0;recursiveResize=0;outlineConnect=0;shadow=0;labelBackgroundColor=none;sketch=1;strokeColor=#6600CC;strokeWidth=1;fillColor=#ffffff;gradientColor=none;fontFamily=Comic Sans MS;fontColor=#4C0099;align=left;size=18;" parent="0uA7_dlgTEhDihHVL7zL-1" vertex="1">
